@article{Kashchiev1982,
	Abstract = {A general thermodynamic relation is proved to exist between the nucleation work and the nucleus size regardless of the model for the excess free energy of the nucleus. When this energy is supersaturation independent, the relation reads as follows: the number of atoms (or molecules) in the nucleus equals the derivative with minus sign of the nucleation work with respect to the supersaturation $\Delta$$\mu$. It is shown that, experimentally, a reliable determination of the nucleus size is possible when data for the stationary nucleation rate J are plotted in coordinates kT ln J vs. $\Delta$$\mu$ (k is the Boltzmann constant and T is the absolute temperature). The slope of such an experimental curve gives information about the nucleus size independently of the kind of nucleation: classical or atomistic, homogeneous or heterogeneous, three dimensional or two dimensional, etc.},
	Author = {Kashchiev, D.},
	Date-Modified = {2013-04-22 17:34:24 +0000},
	Doi = {10.1063/1.442808},
	File = {:Users/u0731152/Documents/Mendeley Desktop/1982/Kashchiev/Kashchiev - 1982 - On the relation between nucleation work, nucleus size, and nucleation rate.pdf:pdf},
	Issn = {00219606},
	Journal = {The Journal of Chemical Physics},
	Keywords = {FREE ENERGY,KINETICS,NUCLEATION,THERMODYNAMICS},
	Number = {10},
	Pages = {5098},
	Title = {{On the relation between nucleation work, nucleus size, and nucleation rate}},
	Url = {http://link.aip.org/link/?JCP/76/5098/1\&Agg=doi},
	Volume = {76},
	Year = {1982},
	Bdsk-Url-1 = {http://link.aip.org/link/?JCP/76/5098/1%5C&Agg=doi},
	Bdsk-Url-2 = {http://dx.doi.org/10.1063/1.442808}}

@article{Kralj1990,
	Abstract = {The kinetics of precipitation of vaterite (CaCO3) from aqueous solution between about 10 and 45$\,^{\circ}$C and at ionic strengths from 15mM to 315mM was determined by recording pH as a function of time and analysing these data by a BASIC computer program, correcting for complexes and using activity coefficients. The particles were spherulites with diameters less than 5 $\mu$m, and the growth kinetics parabolic, with the rate constants (dr/dt)/(S-1)2 equal to 0.180, 0.468, 0.560, 1.250 and 1.860 nm/s at 11.4, 21.7, 25.0, 33.8 and 42.5$\,^{\circ}$C, respectively, and 0.560, 0.588, 0.594 and 0.610 nm/s at ionic strengths 15mM, 65mM, 115mM and 315mM, respectively. The change with temperature corresponds to the activation energy, 57.1 kJ/mol. It is shown that the rate constants found are in quantitative agreement with those calculated from the theory of growth of electrolyte crystals from aqueous solution. This means that the rate-determining mechanism is the release of water molecules from the calcium ions as they ``jump'' into lattice positions.},
	Author = {Kralj, D.},
	Date-Modified = {2013-05-03 18:09:45 +0000},
	Doi = {10.1016/0022-0248(90)90104-S},
	File = {:Users/u0731152/Documents/Mendeley Desktop/1990/Kralj/Kralj - 1990 - Vaterite growth and dissolution in aqueous solution I. Kinetics of crystal growth.pdf:pdf},
	Issn = {00220248},

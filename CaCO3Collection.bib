	Issn = {2040-3372},
	Journal = {Nanoscale},
	Keywords = {Calcium Carbonate,Calcium Carbonate: chemistry,Crystallization,Kinetics,Nanoparticles,Nanoparticles: chemistry,Nanoparticles: ultrastructure,Spectrophotometry, Infrared,Temperature,Thermodynamics,X-Ray Diffraction},
	Number = {1},
	Pages = {265--71},
	Pmid = {21069231},
	Title = {{The kinetics and mechanisms of amorphous calcium carbonate (ACC) crystallization to calcite, via vaterite.}},
